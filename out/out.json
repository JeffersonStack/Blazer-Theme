{
  "name": "Spirited Away",
  "tokenColors": [
    {
      "settings": {
        "background": "#151515",
        "foreground": "#CFCFCF"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#62AD41" //#6d966c
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.character.escape.ts",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#e7ab65"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "constant.other.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.character.escape.backslash.regexp"
      ],
      "settings": {
        "foreground": "#e7ab65"
      }
    },
    {
      "name": "Template expression. e.g. ` '${' '}' `",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#ef6363"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#AFC563"
      }
    },
    {
      "name": "Built-in constant. e.g. 'undefined', 'true', 'false'",
      "scope": "constant.language",
      "settings": {
        "foreground": "#268ac3"
      }
    },
    {
      "name": "'bold', 'italic', 'auto' (CSS)",
      "scope": ["support.constant.property-value"],
      "settings": {
        "foreground": "#e7ab65"
      }
    },
    {
      "name": "Colors (CSS)",
      "scope": [
        "constant.other.color.rgb-value.hex",
        "support.constant.color.w3c-standard-color-name",
        "support.constant.color.w3c-extended-color-name"
      ],
      "settings": {
        "foreground": "#e7ab65"
      }
    },
    {
      "name": "Pseudo class, element (CSS)",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#9977a2"
      }
    },
    {
      "name": "@import, @extend, @mixin (SCSS)",
      "scope": ["keyword.control.at-rule"],
      "settings": {
        "foreground": "#9977a2"
      }
    },
    {
      "name": "Keyword. e.g. 'export', 'return'",
      "scope": "keyword",
      "settings": {
        "foreground": "#ef6363"
      }
    },
    {
      "name": "Storage. e.g. '=', '<<'",
      "scope": "storage",
      "settings": {
        "foreground": "#CFCFCF"
      }
    },
    {
      "name": "Storage modifier. e.g. 'public'",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#268ac3"
      }
    },
    {
      "name": "Imported items",
      "scope": "storage.modifier.import",
      "settings": {
        "foreground": "#ef6363"
      }
    },
    {
      "name": "Object keys",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#CFCFCF"
      }
    },
    {
      "name": "'(' ')' ';' '{' '}' '[' ']' '<' '>' '::'",
      "scope": [
        "punctuation.definition.method-parameters",
        "punctuation.definition.seperator.parameter",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.section",
        "punctuation.definition.block",
        "punctuation.definition.typeparameters",
        "punctuation.definition.begin.bracket.square",
        "punctuation.definition.end.bracket.square",
        "punctuation.definition.binding-pattern.object",
        "punctuation.definition.bracket.curly",
        "punctuation.separator.comma",
        "punctuation.terminator",
        "meta.method.identifier",
        "meta.brace.round",
        "meta.brace.square",
        "punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#ef6363" //#888
      }
    },
    {
      "name": "':' switch-case",
      "scope": "punctuation.definition.section.case-statement",
      "settings": {
        "foreground": "#888"
      }
    },
    {
      "name": "Separators ','",
      "scope": ["punctuation.separator"],
      "settings": {
        "foreground": "#888"
      }
    },
    {
      "name": "'.' method accessor, '->' pointer access",
      "scope": [
        "punctuation.accessor",
        "punctuation.separator.pointer-access",
        "punctuation.separator.dot-access"
      ],
      "settings": {
        "foreground": "#888"
      }
    },
    {
      "name": "Tag definition '< />'",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "name": "'this' 'super'",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#268ac3"
      }
    },
    {
      "name": "Storage type. e.g. 'function', 'let', 'const'",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#268ac3"
      }
    },
    {
      "name": "'regex_t' 'size_t' 'uintmax_t' (C)",
      "scope": [
        "support.type.posix-reserved",
        "support.type.sys-types",
        "support.type.stdint"
      ],
      "settings": {
        "foreground": "#268ac3"
      }
    },
    {
      "name": "Arrow",
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#ef6363"
      }
    },
    {
      "name": "Class name. e.g. 'Map', Action<'T'>",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "entity.other.inherited-class",
        "storage.type.generic",
        "storage.class"
      ],
      "settings": {
        "foreground": "#ef6363"
      }
    },
    {
      "name": "Entity name type",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#ef6363"
      }
    },
    {
      "name": "Function and method name",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.method",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#da846b"
      }
    },
    {
      "name": "Method call and body",
      "scope": ["meta.method-call", "meta.method.body"],
      "settings": {
        "foreground": "#CFCFCF"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ef6363"
      }
    },
    {
      "name": "Attribute name",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#da846b" //#CFCFCF
      }
    },
    {
      "name": "DOCTYPE (HTML)",
      "scope": "meta.tag.sgml.doctype.html",
      "settings": {
        "foreground": "#268ac3"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#CFCFCF"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "meta.definition.variable.name"
      ],
      "settings": {
        "foreground": "#ffffff" //#CFCFCF
      }
    },
    {
      "name": "Variable (SCSS)",
      "scope": "variable.scss",
      "settings": {
        "foreground": "#5cafdf"
      }
    },
    {
      "name": "Property",
      "scope": [
        "support.variable.property",
        "variable.property",
        "variable.object.property",
        "meta.definition.property",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#CFCFCF"
      }
    },
    {
      "name": "Imported item",
      "scope": ["variable.other.readwrite.alias"],
      "settings": {
        "foreground": "#ffffff" // #CFCFCF
      }
    },
    {
      "name": "Enum",
      "scope": ["variable.other.enummember"],
      "settings": {
        "foreground": "#AFC563"
      }
    },
    {
      "name": "Library class/type. e.g. 'string', 'number'",
      "scope": ["support.type", "support.class", "storage.type.core"],
      "settings": {
        "foreground": "#ef6363"
      }
    },
    {
      "name": "Decoratos ('@'Component)",
      "scope": ["punctuation.decorator"],
      "settings": {
        "foreground": "#9977a2"
      }
    },
    {
      "name": "'@'param (jsdoc)",
      "scope": ["punctuation.definition.block.tag"],
      "settings": {
        "foreground": "#268ac3"
      }
    },
    {
      "name": "Macros (C)",
      "scope": ["entity.name.function.preprocessor"],
      "settings": {
        "foreground": "#9977a2"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "e.g. name':' string, string '|' number",
      "scope": "keyword.operator.type",
      "settings": {
        "foreground": "#888"
      }
    },
    {
      "name": "'new', 'of', and other keywords",
      "scope": [
        "keyword.operator.new",
        "keyword.control.new",
        "keyword.operator.expression",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#268ac3"
      }
    },
    {
      "name": "Dereference operator ('.')",
      "scope": "keyword.operator.dereference",
      "settings": {
        "foreground": "#CFCFCF"
      }
    },
    {
      "name": "Markdown header",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#ef6363"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#268ac3"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#268ac3"
      }
    },
    {
      "name": "Markdown list",
      "scope": "beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#ef6363"
      }
    },
    {
      "name": "Markdown link",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#5cafdf"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": ["markup.inline.raw", "punctuation.definition.raw"],
      "settings": {
        "foreground": "#268ac3"
      }
    },
    {
      "name": "Markdown code block",
      "scope": [
        "punctuation.definition.markdown",
        "fenced_code.block.language"
      ],
      "settings": {
        "foreground": "#da846b"
      }
    },
    {
      "name": "Markdown quote",
      "scope": "beginning.punctuation.definition.quote",
      "settings": {
        "foreground": "#6d966c"
      }
    }
  ]
}
